{
  "name": "cursor-time",
  "displayName": "CursorTime",
  "description": "",
  "version": "0.0.2",
  "publisher": "ngwx-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntna141/ntna-time.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "icon": "media/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cursor-time",
          "title": "cursor-time",
          "icon": "media/cursor-time.svg"
        }
      ]
    },
    "views": {
      "cursor-time": [
        {
          "type": "webview",
          "id": "cursor-time.sessionsView",
          "name": "cursor-time"
        }
      ]
    },
    "commands": [
      {
        "command": "cursor-time.showHeartbeats",
        "title": "Show Recent Heartbeats",
        "category": "cursor-time"
      },
      {
        "command": "cursor-time.openPanel",
        "title": "Open Panel",
        "category": "cursor-time"
      },
      {
        "command": "cursor-time.newTodo",
        "title": "New Todo",
        "category": "cursor-time"
      }
    ],
    "keybindings": [
      {
        "command": "cursor-time.openPanel",
        "key": "cmd+shift+i",
        "mac": "cmd+shift+i",
        "win": "ctrl+shift+i",
        "linux": "ctrl+shift+i"
      },
      {
        "command": "cursor-time.newTodo",
        "key": "cmd+u",
        "mac": "cmd+u",
        "win": "ctrl+u",
        "linux": "ctrl+u"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node esbuild.config.js --production",
    "watch": "node esbuild.config.js --watch",
    "install-ext": "./install.sh"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.74.0",
    "esbuild": "^0.27.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0"
  }
}
