{
  "name": "ntna-time",
  "displayName": "ntna-time",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ntna-time",
          "title": "ntna-time",
          "icon": "$(clock)"
        }
      ]
    },
    "views": {
      "ntna-time": [
        {
          "type": "webview",
          "id": "ntna-time.sessionsView",
          "name": "Sessions"
        }
      ]
    },
    "commands": [
      {
        "command": "ntna-time.showHeartbeats",
        "title": "Show Recent Heartbeats",
        "category": "ntna-time"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node esbuild.config.js --production",
    "watch": "node esbuild.config.js --watch"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.74.0",
    "esbuild": "^0.27.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0"
  }
}
